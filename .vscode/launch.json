  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {
        "CA_KEY": "ssl/private/ca-key.pem",
        "CA_CERT": "ssl/certs/ca-cert.pem",
      },
      "args": [
        "--config=config.toml",
      ]
    }
  ]
}