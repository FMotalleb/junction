  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {
        "ZAPLOG_DEVELOPMENT": "true",
        "ZAPLOG_LEVEL": "debug",
        "ZAPLOG_LEVEL_FORMAT": "color",
        "TIMEOUT": "5h"
      },
      "args": [
        "dump",
        "--config=config.toml",
      ]
    }
  ]
}